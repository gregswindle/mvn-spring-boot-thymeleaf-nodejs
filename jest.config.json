{
	"name": "spring-boot-starter-thymeleaf",
	"automock": false,
	"collectCoverage": true,
	"coverageDirectory": "./coverage/",
	"coveragePathIgnorePatterns": [
		"/docs/",
		"/node_modules/",
		"/site/",
    "/src/test/resources/",
    "/target/"
	],
	"coverageThreshold": {
		"global": {
			"branches": 90,
			"functions": 90,
			"lines": 90,
			"statements": 90
		}
	},
	"testPathIgnorePatterns": [
		"/target/"
	],
	"verbose": true
}
